{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/clients": [
        "libs/clients/src"
      ],
      "@app/clients/*": [
        "libs/clients/src/*"
      ],
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/contracts": [
        "libs/contracts/src"
      ],
      "@app/contracts/*": [
        "libs/contracts/src/*"
      ],
      "@app/counters": [
        "libs/counters/src"
      ],
      "@app/counters/*": [
        "libs/counters/src/*"
      ],
      "@app/exceptions": [
        "libs/exceptions/src"
      ],
      "@app/exceptions/*": [
        "libs/exceptions/src/*"
      ],
      "@app/handlers": [
        "libs/handlers/src"
      ],
      "@app/handlers/*": [
        "libs/handlers/src/*"
      ],
      "@app/ports": [
        "libs/ports/src"
      ],
      "@app/ports/*": [
        "libs/ports/src/*"
      ],
      "@app/proto": [
        "libs/proto/src"
      ],
      "@app/proto/*": [
        "libs/proto/src/*"
      ],
      "@app/utils": [
        "libs/utils/src"
      ],
      "@app/utils/*": [
        "libs/utils/src/*"
      ],
      "@auth/*": [
        "apps/auth/src/*"
      ],
      "@authz/*": [
        "apps/authz/src/*"
      ],
      "@channel/*": [
        "apps/channel/src/*"
      ],
      "@cloud/*": [
        "apps/cloud/src/*"
      ],
      "@comments/*": [
        "apps/comments/src/*"
      ],
      "@email/*": [
        "apps/email/src/*"
      ],
      "@gateway/*": [
        "apps/api-gateway/src/*"
      ],
      "@history/*": [
        "apps/history/src/*"
      ],
      "@peristance/channel": [
        "apps/channel/generated/prisma"
      ],
      "@peristance/comments": [
        "apps/comments/generated/prisma"
      ],
      "@peristance/reaction": [
        "apps/reaction/generated/prisma"
      ],
      "@peristance/user": [
        "apps/users/generated/prisma"
      ],
      "@peristance/videos": [
        "apps/videos/generated/prisma"
      ],
      "@persistance/auth": [
        "apps/auth/generated/prisma"
      ],
      "@persistance/views": [
        "apps/views/generated/prisma"
      ],
      "@playlist/*": [
        "apps/playlist/src/*"
      ],
      "@projection/*": [
        "apps/projection/src/*"
      ],
      "@query/*": [
        "apps/query/src/*"
      ],
      "@reaction/*": [
        "apps/reaction/src/*"
      ],
      "@subscribe/*": [
        "apps/subscribe/src/*"
      ],
      "@transcoder/*": [
        "apps/video-transcoder/src/*"
      ],
      "@users/*": [
        "apps/users/src/*"
      ],
      "@videos/*": [
        "apps/videos/src/*"
      ],
      "@views/*": [
        "apps/views/src/*"
      ]
    }
  }
}